<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<display-name>login</display-name>
  	<description></description>
  	<servlet-class>virtusalogin.login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DliLogin</servlet-name>
  	<display-name>DliLogin</display-name>
  	<description></description>
  	<servlet-class>virtusalogin.DliLogin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Invorder</servlet-name>
  	<display-name>Invorder</display-name>
  	<description></description>
  	<servlet-class>orders.Invorder</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AssignedOrders</servlet-name>
  	<display-name>AssignedOrders</display-name>
  	<description></description>
  	<servlet-class>orders.AssignedOrders</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/inv_login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DliLogin</servlet-name>
  	<url-pattern>/dlogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Invorder</servlet-name>
  	<url-pattern>/assign</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AssignedOrders</servlet-name>
  	<url-pattern>/Orders</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>order</servlet-name>
  <servlet-class>orders.AssignedOrders</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>order</servlet-name>
  <url-pattern>/order</url-pattern>
  </servlet-mapping>
  
</web-app>
